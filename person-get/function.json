{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "persion/{id}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "queryPerson",
      "databaseName": "outDatabase",
      "collectionName": "MyCollection",
      "connectionStringSetting": "connectionStringSetting",
      "Id": "{id}"
    }
  ]
}
